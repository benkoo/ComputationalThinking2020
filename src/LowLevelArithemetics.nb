     RowBox[{"digitCount", " ", "=", " ", 
      RowBox[{"Length", "[", "yDigits", "]"}]}], ";", "\n", "  ", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "digitCount"}], ",", " ", 
